import java.util.*;
public static void important_Q {
    public static void main(string[] args) {
        Scanner sc = new Scanner(System.in);
        String s = sc.next();
        s = s.tolowerCase();
        int[] b = new int[26];
        for(int i = 0; i<s.length(); i++){
            b[s.charAt(i)-'a']++;
            
        }
        for(int i=0; i<b.length; i++){
            if (b[i]>0)
            System.out.printf("%c = %d\n", i+97 , b[i];
        }
    }
}    
